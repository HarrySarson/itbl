{
  "name": "itbl",
  "version": "2.2.3",
  "description": "Utility libary for es6 iterators",
  "main": "itbl.js",
  "scripts": {
    "preversion": "npm test && npm run style",
    "version": "npm run doc && npm run test:doc && git add --all --force docs",
    "style": "npm run style:main",
    "style:main": "jscs itbl.js",
    "style:test": "jscs test/test.babel.js",
    "test": "npm run test:main",
    "test:doc": "markdown-doctest docs/*.md",
    "test:main": "node compiled/test/test.babel.js",
    "remove-generated-files": "echo \"Removing files generated by itbl\" && shx rm compiled docs coverage",
    "compile": "npm run compile:main && npm run compile:lib && npm run compile:test",
    "compile:main": "mkdirp compiled && babel itbl.js --out-dir compiled",
    "compile:lib": "mkdirp compiled/test && babel lib --out-dir compiled/lib",
    "compile:test": "mkdirp compiled/test && babel test --out-dir compiled/test",
    "istanbul": "istanbul cover compiled/test/test.babel.js",
    "codecov": "codecov",
    "doc": "npm run doc:site && npm run doc:md && npm run test:doc",
    "doc:site": "babel-node lib/preprocess-html && jsdoc -c .jsdoc.json && babel-node lib/postprocess-html && shx rm -r docs/replaced ",
    "doc:md": "babel-node lib/preprocess-md && node lib/build-md && shx rm -r docs/replaced"
  },
  "keywords": [
    "util",
    "es6",
    "iterator"
  ],
  "repository": "https://github.com/HarrySarson/itbl",
  "author": "Harry Sarson",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.1.8",
    "babel-plugin-transform-runtime": "^6.22.0",
    "bluebird": "^3.4.1",
    "codecov": "^1.0.1",
    "docdown": "^0.7.1",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "itbl": "1.1.2",
    "jscs": "^3.0.7",
    "lodash": "^4.16.3",
    "mkdirp": "^0.5.1",
    "markdown-doctest": "^0.9.1",
    "minami": "^1.1.1",
    "mkdirp": "^0.5.1",
    "qunit-extras": "^2.1.0",
    "qunitjs": "^2.1.1",
    "replace-in-file": "2.0.3",
    "shx": "^0.2.2"
  }
}
